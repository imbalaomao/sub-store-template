      },
      {
        "rule_set": "cn",
        "server": "tx"
      }
    ],
    "final": "google",
    "independent_cache": true
  },
  "route": {
    "rules": [
      {
        "port": 53,
        "outbound": "dns-out"
      },
      {
        "ip_is_private": true,
        "outbound": "direct"
      },
      {
        "clash_mode": "Direct",
        "outbound": "direct"
      },
      {
        "clash_mode": "Global",
        "outbound": "GLOBAL"
      },
      {
        "rule_set": "bilibili",
        "outbound": "direct"
      },
      {
        "rule_set": "netflix",
        "outbound": "proxy"
      },
      {
        "rule_set": "bahamut",
        "outbound": "proxy"
      },
      {
