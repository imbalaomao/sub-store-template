  "outbounds": [
    {
      "tag": "proxy",
      "type": "selector",
      "outbounds": [
        "all",
        "all-auto",
        "direct"
      ],
      "default": "all-auto"
    },
    {
      "tag": "final",
      "type": "selector",
      "outbounds": [
        "all",
        "all-auto"
      ],
      "default": "proxy"
    },
    {
      "tag": "all-auto",
      "type": "urltest",
      "outbounds": [],
      "url": "https://www.gstatic.com/generate_204",
      "interval": "1m",
      "tolerance": 50
    },
    {
      "tag": "GLOBAL",
      "type": "selector",
      "outbounds": [
        "direct",
        "all",
        "all-auto"
      ],
      "default": "direct"
